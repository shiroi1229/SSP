{
  "servers": {
    "api-explorer": {
      "command": "C:/Python313/python.exe",
      "args": [
        "-m",
        "api_explorer_server.server",
        "stdio"
      ]
    },
    "api-inspector": {
      "command": "npm",
      "args": [
        "--prefix",
        "mcp/api-inspector",
        "run",
        "start"
      ]
    },
    "dbhub": {
      "command": "npx",
      "args": [
        "-y",
        "@bytebase/dbhub",
        "--transport",
        "stdio",
        "--demo"
      ]
    },
    "error-analyzer": {
      "command": "npm",
      "args": [
        "--prefix",
        "mcp/error-analyzer",
        "run",
        "start"
      ]
    },
    "everything": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-everything"
      ]
    },
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-server-filesystem",
        "d:/gemini"
      ]
    },
    "filesystem-gemini": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-server-filesystem",
        "d:/gemini"
      ]
    },
    "git": {
      "command": "uvx",
      "args": [
        "mcp-server-git"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "modules-inspector": {
      "command": "npm",
      "args": [
        "--prefix",
        "mcp/modules-inspector",
        "run",
        "start"
      ]
    },
    "openapi-mcp-server": {
      "command": "npx",
      "args": [
        "-y",
        "wrangler",
        "dev",
        "--config",
        "mcp/openapi-mcp-server/wrangler.json"
      ]
    },
    "sequentialthinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "ssp-mcp": {
      "command": "node",
      "args": [
        "mcp/ssp-server.mjs"
      ],
      "env": {
        "SSP_API_BASE_URL": "https://ssp.example.internal/api"
      }
    },
    "starwind-ui": {
      "command": "npm",
      "args": [
        "--prefix",
        "mcp/starwind-ui-mcp",
        "run",
        "start"
      ]
    },
    "terminal-controller": {
      "command": "C:/Python313/python.exe",
      "args": [
        "-m",
        "terminal_controller",
        "--transport",
        "stdio"
      ]
    },
    "test-orchestrator": {
      "command": "npm",
      "args": [
        "--prefix",
        "mcp/test-orchestrator",
        "run",
        "start"
      ]
    },
    "time": {
      "command": "uvx",
      "args": [
        "mcp-server-time"
      ]
    },
    "ui-map": {
      "command": "npm",
      "args": [
        "--prefix",
        "mcp/ui-map",
        "run",
        "start"
      ]
    }
  }
}
